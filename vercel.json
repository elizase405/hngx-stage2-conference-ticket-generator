{
	"version": 2,
	"builds": [
		{
			"src": "/api/json-server.js",
			"use": "@vercel/node",
			"config": {
				"includeFiles": ["db.json"]
			}
		}
	],
	"routes": [
		{
			"src": "/api/(.*)",
			"dest": "/api/json-server.js"
		}
	],
	"rewrites": [
		{
			"sources": "/(.*)",
			"destination": "/"
		}
	]
}